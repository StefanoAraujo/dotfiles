{
    "coc.preferences.hoverTarget": "float",
    "suggest.floatEnable": true,
    "suggest.maxPreviewWidth": 40,
    "suggest.enablePreview": true,
    "list.maxHeight": 10,
    "list.maxPreviewHeight": 10,
    "suggest.labelMaxLength": 20,
    "suggest.removeDuplicateItems": true,
    "java.import.gradle.enabled": true,
    "java.import.maven.enabled": true,
    "java.home": "/usr/lib/jvm/jdk1.8.0_251",
    "java.configuration.updateBuildConfiguration": "automatic",
    "java.import.gradle.wrapper.enabled": true,
    "java.autobuild.enabled": true,
    "javascript.suggest.autoImports": true,
    "java.format.enabled": true,
    "java.referencesCodeLens.enabled": true,
    "java.signatureHelp.enabled": true,
    "java.foldingRange.enabled": true,
    "java.format.comments.enabled": true,
    "java.completion.guessMethodArguments": true,
    "java.format.onType.enabled": true,
    "java.codeGeneration.useBlocks": true,
    "explorer.previewAction.onHover": false,
    "coc.preferences.rootPatterns": [
        ".git",
        ".env",
        "queroemprestar",
        "dashboard",
        "src",
        "django_country_filter",
        "tests"
    ],
    "java.errors.incompleteClasspath.severity": "ignore",
    "java.maven.downloadSources": true,
    "java.jdt.ls.vmargs": "-javaagent:/home/paulo/tools/lombok.jar -Xbootclasspath/a:/home/paulo/tools/lombok.jar",
    "python.autoUpdateLanguageServer": true,
    "python.linting.enabled": true,
    "python.jediEnabled": false,
    "languageserver": {
        "golang": {
            "command": "gopls",
            "disableWorkspaceFolders": true,
            "filetypes": [
                "go"
            ]
        },
        "clangd": {
            "command": "clangd",
            "rootPatterns": [
                "compile_flags.txt",
                "compile_commands.json"
            ],
            "filetypes": [
                "c",
                "cc",
                "cpp",
                "c++",
                "objc",
                "objcpp"
            ]
        },
        "python": {
            "command": "python",
            "args": [
                "-mpyls",
                "-vv",
                "--log-file",
                "/tmp/lsp_python.log"
            ],
            "trace.server": "verbose",
            "filetypes": [
                "python"
            ],
            "settings": {
                "pyls": {
                    "enable": true,
                    "trace": {
                        "server": "verbose"
                    },
                    "commandPath": "",
                    "configurationSources": [
                        "pycodestyle"
                    ],
                    "plugins": {
                        "jedi_completion": {
                            "enabled": false
                        },
                        "jedi_hover": {
                            "enabled": false
                        },
                        "jedi_references": {
                            "enabled": false
                        },
                        "jedi_signature_help": {
                            "enabled": false
                        },
                        "jedi_symbols": {
                            "enabled": false,
                            "all_scopes": false
                        },
                        "mccabe": {
                            "enabled": false,
                            "threshold": 15
                        },
                        "preload": {
                            "enabled": true
                        },
                        "pycodestyle": {
                            "enabled": true
                        },
                        "pydocstyle": {
                            "enabled": true,
                            "match": "(?!test_).*\\.py",
                            "matchDir": "[^\\.].*"
                        },
                        "pyflakes": {
                            "enabled": true
                        },
                        "rope_completion": {
                            "enabled": false
                        },
                        "yapf": {
                            "enabled": false
                        }
                    }
                }
            }
        }
    },
    "explorer.keyMappings": {
        "i": false,
        "gk": "expandablePrev",
        "gj": "expandableNext",
        "*": "toggleSelection",
        "<tab>": "actionMenu",
        "h": "collapse",
        "l": [
            "expandable?",
            "expand",
            "open"
        ],
        "J": [
            "toggleSelection",
            "nodeNext"
        ],
        "K": [
            "toggleSelection",
            "nodePrev"
        ],
        "gl": "expandRecursive",
        "gh": "collapseRecursive",
        "<2-LeftMouse>": [
            "expandable?",
            [
                "expanded?",
                "collapse",
                "expand"
            ],
            "open"
        ],
        "o": [
            "expanded?",
            "collapse",
            "expand"
        ],
        "<cr>": [
            "expandable?",
            "cd",
            "open"
        ],
        "e": "open",
        "s": "open:split",
        "S": "open:split:plain",
        "E": "open:vsplit",
        "t": "open:tab",
        "<bs>": "gotoParent",
        "gp": "preview:labeling",
        "y": "copyFilepath",
        "Y": "copyFilename",
        "c": "copyFile",
        "x": "cutFile",
        "p": "pasteFile",
        "d": "delete",
        "D": "deleteForever",
        "a": "addFile",
        "A": "addDirectory",
        "r": "rename",
        ".": "toggleHidden",
        "R": "refresh",
        "?": "help",
        "q": "quit",
        "<esc>": "esc",
        "X": "systemExecute",
        "gd": "listDrive",
        "f": "search",
        "F": "searchRecursive",
        "gf": "gotoSource:file",
        "gb": "gotoSource:buffer",
        "[[": "sourcePrev",
        "]]": "sourceNext",
        "[d": "diagnosticPrev",
        "]d": "diagnosticNext",
        "[c": "gitPrev",
        "]c": "gitNext",
        "<<": "gitStage",
        ">>": "gitUnstage"
    }
}
